{
  "name": "image-to-geo-location",
  "env": {
      "GOOGLE_CLOUD_PROJECT": {
          "description": "Your Google Cloud project ID",
          "order": 100      
      },
      "GOOGLE_CLOUD_REGION": {
          "description": "The Google Cloud region you are deploying to",
          "value": "us-central1",
          "required": false,
          "order": 150
      }
  },
  "options": {
      "allow-unauthenticated": true,
      "memory": "512Mi",
      "cpu": "1",
      "concurrency": 500,
      "max-instances": 4
  }
}