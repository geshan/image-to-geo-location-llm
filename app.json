{
  "name": "image-to-geo-location-llm",
  "env": {
      "GOOGLE_CLOUD_REGION": {
          "description": "The Google Cloud region you are deploying to",
          "order": 150
      }
  },
  "options": {
      "allow-unauthenticated": true,
      "memory": "512Mi",
      "cpu": "1",
      "concurrency": 500,
      "max-instances": 4
  },
  "hooks": {
      "prebuild": {
          "commands": [
              "export GOOGLE_CLOUD_PROJECT=$(gcloud config get-value project)"
          ]
      }
  }    
}